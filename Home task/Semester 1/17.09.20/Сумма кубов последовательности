#include <iostream>
//Вывести среднее арифметическое кубов чисел последовательности заданной длины n
using namespace std;

int main()
{
	int n;

	double num, sum = 0;

	cin >> n;

	for (int i = 0; i < n; i++)
	{
		cin >> num;

		sum += (num * num * num);
	}

	sum = sum / n;

	cout << sum;

	return 0;
}
